<component name="uiDialog" extends="baseUIComponent" >

  <script type="text/brightscript" uri="pkg:/components/UIComponents/UIDialog/UIDialog.brs" />

  <interface>
     <field id="parentFocusItem" type="node" />
     <field id="close" type="boolean" onChange="onClose" />
